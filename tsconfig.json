{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "isolatedModules": true,
    "types": [
      "node",
      "electron-vite/node"
    ]
  },
  "include": [
    "electron.vite.config.ts",
    "src/**/*"
  ]
}
